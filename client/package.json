      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.0.5",
    "autoprefixer": "^9.7.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "postcss-cli": "^6.1.3",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.1.3"
